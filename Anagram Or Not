package something;
import java.util.Arrays;
import java.util.*;

public class ImportantStringprogram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String x="silent";
		String y="listen";
		char a[]=x.toCharArray();
		char b[]=y.toCharArray();
		Arrays.sort(a);
		Arrays.sort(b);
		Boolean result=Arrays.equals(a,b);
		if(result==true)
		{
			System.out.println("Anagram");
		}
		else
		{
			System.out.println("nahi yaar anagram nahi ha ye jaa mara");
		}
		

	}

}
